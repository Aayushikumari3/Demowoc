<div class="row">
    <div class="col-md-12">
      <h1>Live Canteen Status</h1>
      <table class="table">
        <thead>
          <tr>
            <th>Item Name</th>
            <th>Available Quantity</th>
            <th>Total Quantity</th>
            <th>Price</th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < items.length; i++) { %>
          <tr>
            <td><%= items[i].name %></td>
            <td><%= items[i].available_quantity %></td>
            <td><%= items[i].total_quantity %></td>
            <td><%= items[i].price %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
  